<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="hBox" alignment="CENTER" minHeight="40.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="cs2410.assn8.controller.HBoxController">
  <children>
    <VBox fx:id="leftVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
      <children>
        <Text fx:id="bombsLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Bombs Left" />
        <Text fx:id="bombsTickLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      </children>
    </VBox>
    <Button fx:id="startBtn" mnemonicParsing="false" text="Start" />
    <VBox fx:id="rightVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
      <children>
        <Text fx:id="timeLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Time" />
        <Text fx:id="timeTickLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      </children>
    </VBox>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
</HBox>
